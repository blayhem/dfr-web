    "next-optimized-images": "^2.6.2",
    "prop-types": "^15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "remark-images": "^2.0.0",
    "sass": "^1.26.10",
    "webp-loader": "^0.6.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.12.1",
    "@next/bundle-analyzer": "^9.3.6",
